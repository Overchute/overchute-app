{
  "canisters": {
    "counter": {
      "main": "counter-canister/main.mo",
      "type": "motoko"
    },
    "crowdsale": {
      "main": "crowdsale-canister/main.mo",
      "type": "motoko"
    },
    "assets": {
      "dependencies": [
        "counter"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": [
        "dist/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": "vessel sources"
    }
  },
  "dfx": "0.8.1",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}