{
  "canisters": {
    "crowdsale": {
      "dependencies": [
        "ledger"
      ],
      "main": "crowdsale-canister/main.mo",
      "type": "motoko"
    },
    "ledger": {
      "type": "custom",
      "candid": "src/ledger/ledger.did",
      "wasm": "src/ledger/ledger.wasm"
    },
    "assets": {
      "dependencies": [
        "crowdsale"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": [
        "dist/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": "vessel sources"
    }
  },
  "dfx": "0.8.5",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}